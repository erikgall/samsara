{
    "name": "erikgall/samsara",
    "description": "Laravel SDK for the Samsara Fleet Management API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Samsara\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Samsara\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Erik Galloway",
            "email": "8866568+erikgall@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/collections": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.21",
        "phpunit/phpunit": "^10.0|^11.0",
        "phpstan/phpstan": "^2.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Samsara\\SamsaraServiceProvider"
            ],
            "aliases": {
                "Samsara": "Samsara\\Facades\\Samsara"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    }
}
